# uvicorn
uvicorn_app=api.app:xingchen_rap_server_app
uvicorn_host=0.0.0.0
uvicorn_port=19999
uvicorn_workers=20
uvicorn_reload=false
uvicorn_ws_ping_interval=20.0
uvicorn_ws_ping_timeout=20.0

# log
log_level=INFO
log_path=./logs/rpa_server.log

# rpa
rpa_timeout=3000
rpa_ping_interval=3
rpa_task_create_url=https://newapi.iflyrpa.com/api/rpa-openapi/workflows/execute-async
rpa_task_query_url=https://newapi.iflyrpa.com/api/rpa-openapi/executions
rpa_task_query_interval=10