# Link Development Environment Configuration File Example
# Copy this file to config.env and modify configuration values according to your actual environment
# Note: config.env file contains sensitive information and should not be committed to version control

# Python Environment Configuration
PYTHONUNBUFFERED=1

# Polaris Configuration Center
polaris_url=http://YOUR_POLARIS_HOST:8090
polaris_cluster=dev
polaris_username=YOUR_USERNAME
polaris_password=YOUR_POLARIS_PASSWORD

# link-sid
SERVICE_SUB=sparklink
SERVICE_LOCATION=dev
SERVICE_PORT=18888

# spark-log
spark_link_log_level=INFO
spark_link_log_path=./logs/spark-link.log
spark_run=dev

# uvicorn
uvicorn_host=0.0.0.0
uvicorn_port=18888
uvicorn_workers=20
uvicorn_reload=false
uvicorn_ws_ping_interval=null
uvicorn_ws_ping_timeout=null

# black list
segment_black_list=
ip_black_list=
domain_black_list=

# default appid
app_id=

# generate id
datacenter_id=1
worker_id=1

# tool type
official_tool=official
third_tool=third

# mysql
mysql_host=
mysql_port=
mysql_user=
mysql_password=
mysql_db=

# redis
redis_is_cluster=
redis_cluster_addr=
redis_password=

# otlp
enable_otlp=true

# Kafka
KAFKA_SERVERS=
KAFKA_TIMEOUT=
KAFKA_TOPIC_SPARKLINK_LOG_TRACE=